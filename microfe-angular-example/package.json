{
    "name": "microfe-react-example",
    "version": "1.0.0",
    "private": true,
    "license": "ISC",
    "main": "index.js",
    "workspaces": [
        "projects/*"
    ],
    "resolutions": {
        "html-webpack-plugin": "4.5.1",
        "webpack": "5.15.0"
    },
    "scripts": {
        "c": "git-cz",
        "init": "lerna init",
        "bootstrap": "lerna bootstrap",
        "dev": "lerna run --parallel dev --stream",
        "dev:filter": "lerna run --parallel dev --scope mfe-shell --scope mfe-app-profile",
        "dev:shell": "lerna run dev --scope mfe-shell --stream",
        "dev:profile": "lerna run  dev --scope mfe-app-profile --stream",
        "build": "lerna run --parallel build",
        "serve": "serve ./dist/microfe-angular-example -p 4200"
    },
    "config": {
        "commitizen": {
            "path": "./microfe-angular-example/node_modules/cz-lerna-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "commitizen": "^4.2.3",
        "cz-lerna-changelog": "^2.0.3",
        "husky": "^5.1.3",
        "lerna": "^3.22.1",
        "serve": "^11.3.2"
    }
}